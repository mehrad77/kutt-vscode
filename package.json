{
	"name": "kutt",
	"displayName": "Kutt.it - Free & Open Source Modern URL Shortener",
	"description": "Shorten your links in VSCode ",
	"author": {
		"name": "Mehrad Rousta",
		"email": "mehrad77@gmail.com"
	},
	"publisher": "mehrad77",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.kutt"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.kutt",
				"title": "Kutt"
			}
		],
		"configuration": [
			{
				"title": "Kutt Configurations",
				"properties": {
					"conf.API_KEY": {
						"type": "string",
						"default": "",
						"description": "API token of kutt.it",
						"scope": "resource"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"keywords": [
		"link",
		"link shortner"
	],
	"dependencies": {
		"axios": "^0.19.0"
	}
}
